<defs>
	<clipPath id="overview-clip" clipPathUnits="userSpaceOnUse">
		<rect
			x="0"
			y="0"
			width="<%= overview.width %>"
			height="<%= overview.height %>"
			rx="<%= overview.height / 2 %>"
			ry="<%= overview.height / 2 %>"
		/>
	</clipPath>
</defs>
<% overview.groups.forEach(group => { %>
	<rect
		x="<%= group.x %>"
		y="0"
		width="<%= group.width %>"
		height="<%= overview.height %>"
		fill="<%= group.color %>"
		clip-path="url(#overview-clip)"
	/>
<% }); %>
<% overview.groups.forEach(group => { %>
	<% if (group.label.type == 'left') { %>
		<% group.label.text.forEach((line, i) => { %>
			<text
				x="0"
				y="<%= overview.height + overview.text_gap + i * overview.line_height %>"
				fill="<%= group.color %>"
				stroke="<%= group.color %>"
				font-family="<%= font.name %>"
				font-size="<%= overview.font_size %>"
				stroke-width="<%= overview_text_stroke %>"
				text-anchor="start"
				alignment-baseline="hanging"
				dominant-baseline="hanging"
			>
				<%= line %>
			</text>
		<% }); %>
	<% } %>
<% }); %>
<% overview.groups.forEach(group => { %>
	<% if (group.label.type == 'right') { %>
		<% group.label.text.forEach((line, i) => { %>
			<text
				x="<%= overview.width %>"
				y="<%= overview.height + overview.text_gap + i * overview.line_height %>"
				fill="<%= group.color %>"
				stroke="<%= group.color %>"
				font-family="<%= font.name %>"
				font-size="<%= overview.font_size %>"
				stroke-width="<%= overview_text_stroke %>"
				text-anchor="end"
				alignment-baseline="hanging"
				dominant-baseline="hanging"
			>
				<%= line %>
			</text>
		<% }); %>
	<% } %>
<% }); %>
